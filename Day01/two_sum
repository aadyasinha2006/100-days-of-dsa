#include <stdio.h>
int main()
{
    int n, pos, x;
    int arr[100]; //assuming max size 100

    //input
    scanf("%d", &n);

    for (int i = 0; i < n; i++)
    {
        scanf("%d", &arr[i]);
    }

    scanf("%d", &pos);
    scanf("%d", &x);

    //shift elements to the right from the end to pos-1
    for (int i = n; i >= pos; i--)
    {
        arr[i] = arr[i-1];
    }

    //insert element
    for (int i = 0; i <= n; i++)
    {
        printf("%d", arr[i]);
    }
    return 0;
}